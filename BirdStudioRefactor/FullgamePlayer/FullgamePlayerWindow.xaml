<Window x:Class="BirdStudioRefactor.FullgamePlayerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BirdStudioRefactor"
        mc:Ignorable="d"
        Title="birb" Height="800" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <local:FileQueue x:Name="fileQueue" Grid.Row="0" />
        <Button Click="Open_Click" Grid.Row="1">Open TAS</Button>
        <Button Click="Save_Click" Grid.Row="2">Save TAS</Button>
        <Button Click="AddFile_Click" Grid.Row="3">Add File</Button>
        <Button Click="RemoveFile_Click" Grid.Row="4">Remove File(s)</Button>
        <Button Click="Force_Click" Grid.Row="5">Force/Unforce</Button>
    </Grid>
</Window>
