<UserControl x:Class="BirdStudioRefactor.TASEditorHeader"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:BirdStudioRefactor"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <DockPanel>
            <Label>Stage:</Label>
            <TextBox Name="stageInput" HorizontalAlignment="Stretch" />
        </DockPanel>
        <DockPanel>
            <Label>Spawn:</Label>
            <TextBox Name="spawnInput" />
        </DockPanel>
        <DockPanel>
            <Label>Rerecords:</Label>
            <TextBox Name="rerecordsInput" />
        </DockPanel>
    </StackPanel>
</UserControl>
